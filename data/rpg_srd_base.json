    "attributes": ["forca", "destreza", "constituicao", "inteligencia", "sabedoria", "carisma"],
    "sizes": ["pequeno", "medio"],
    "spell_progression_types": ["full", "half", "third", "pact", "none"]
  },

  "trait_dictionary": {
    "t_olhos_profundezas": {
